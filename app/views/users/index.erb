<div class="jumbotron">
  <h1 class="display-4">ALL USERS</h1>
</div>

<div class="container">
  <div class="row">
    <% @users.each do |user|  %>
      <div class="col-lg-4">
        <h3><%= link_to user.name, user_path(user), html: { class:"btn btn-primary" } %></h3>
        <p>Some quick example text to build on the card title and make up the bulk of the card's content.</p>
      </div>
    <% end %>
  </div>
</div>
